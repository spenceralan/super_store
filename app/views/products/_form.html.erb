<%= render "layouts/errors", :object => @link %>

<%= form_for @link do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :price %>
  <%= f.text_field :price, class: "form-control" %>
  <%= f.label :country %>
  <%= f.text_field :country, class: "form-control" %><br>
  <%= f.submit class: "btn btn-secondary" %>
<% end %>
